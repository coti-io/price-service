{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nest Debug Npm",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug"],
      "console": "integratedTerminal",
      "protocol": "inspector",
      "port": 9229,
      "restart": false,
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"],
      "timeout": 50000
    }
  ]
}
